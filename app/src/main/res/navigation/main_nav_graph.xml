<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.surveasy.surveasy.presentation.main.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/listFragment"
        android:name="com.surveasy.surveasy.presentation.main.list.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" />
    <fragment
        android:id="@+id/myFragment"
        android:name="com.surveasy.surveasy.presentation.main.my.MyFragment"
        android:label="MyFragment" />
    <action
        android:id="@+id/action_global_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_global_listFragment"
        app:destination="@id/listFragment" />
    <action
        android:id="@+id/action_global_myFragment"
        app:destination="@id/myFragment" />
</navigation>